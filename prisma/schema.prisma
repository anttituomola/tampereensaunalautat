// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lautta {
  id String @id @default(cuid())
  name String
  url_name String? @unique
  location String
  capacity Int
  pricemin Int
  pricemax Int?
  equipment String[]
  images String[]
  mainImage String
  email String?
  phone String?
  url String?
  notes String?
}